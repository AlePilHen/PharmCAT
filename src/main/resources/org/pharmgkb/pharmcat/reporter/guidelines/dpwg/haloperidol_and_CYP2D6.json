      "genePhenotypes": [
        "CYP2D6:No function/No function"
      ],
      "metabolizerStatus": {
        "id": 1452057583,
        "html": "<p>Poor Metabolizer</p>\n"
      },
      "recommendation": {
        "id": 1452057584,
        "html": "<p>Use 60% of the normal dose.</p>\n"
      },
      "implications": {
        "id": 1452057582,
        "html": "<p>There are indications for an increased risk of side effects. The genetic variation leads to decreased conversion of haloperidol, resulting in plasma concentrations that are approximately 1.7-fold higher.</p>\n"
      },
      "activityScore": {
        "id": 1452057581,
        "html": "<p>0</p>\n"
      }
