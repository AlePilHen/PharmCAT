    }

    .rxChange a:link, .rxChange a:visited {
      color: darkred;
      font-weight: bold;
    }

    .rxChange {
      color: darkred;
    }

    .rxPossibly a:link, .rxPossibly a:visited {
      color: darkorange;
      font-weight: bold;
    }

    .rxPossibly {
      color: darkorange;
    }

    .highlightDrug a:link, .highlightDrug a:visited {
      color: #3a87ad;
      font-weight: bold;
    }

    .highlight {
      color: #3a87ad;
    }

